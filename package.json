{
  "name": "nextarter-chakra",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "turbo run build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "npx turbo lint type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "openapi:codegen": "openapi -i https://healthrekdb-22326.nodechef.com/swagger/v1/swagger.json -o ./src/services -c axios --useOptions"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^2.9.11",
    "axios": "^1.6.3",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dayjs": "^1.11.10",
    "form-data": "^4.0.0",
    "framer-motion": "^10.16.4",
    "next": "^14.0.1",
    "next-client-cookies": "^1.1.0",
    "next-pwa": "^5.6.0",
    "next13-progressbar": "^1.1.3",
    "openapi-typescript-codegen": "^0.25.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-csv-downloader": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-multi-date-picker": "^4.4.1",
    "use-debounce": "^10.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/cz-commitlint": "^18.2.0",
    "@playwright/test": "^1.39.0",
    "@types/react": "^18.2.37",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.1",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  }
}
